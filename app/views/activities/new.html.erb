<h1> New Activity </h1>
<h3> New Activity Form </h3>
<%= form_for @activity do |f| %>
    <div>
        <%= f.label :name%>
        <%= f.text_field :name%>
    </div>
    <div>
        <%= f.label :duration%>
        <%= f.text_field :duration%>
    </div>
    <div>
        <%= f.label :category%>
        <%= f.text_field :category%>
    </div>
    
   
         <%=f.submit "Submit"%>
    <%end %>

    
