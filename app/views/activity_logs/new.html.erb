
<h3> New Activity Log Form </h3>
<%= form_for  @activity_log do |f| %>

<div>   
    <%= f.collection_select(:activity_id, Activity.all, :id, :name, { prompt: @activity.name  }, class: "browser-default") %> <br/><br/>
<div>
    <%= f.label :duration %>
    <%= f.text_field :duration %>
   
         <%=f.submit "Submit"%>
<%end %>

    
